pipeline{
    agent any
        stages{
            stage("Checkout"){
                step {
                    echo "this step will checkout the code"
                }
               
            }
            stage("Build"){
                step{
                    echo "This step is to build the app"
                }
            }
            stage("deploy"){
                step{
                    echo "This step is to deloy the app"
                }
            }
        }
    
}
